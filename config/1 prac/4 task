#!/bin/bash

# Проверяем, что был передан аргумент с файлом
if [ -z "$1" ]; then
    echo "Использование: $0 <имя файла>"
    exit 1
fi

# Проверяем, что файл существует
if [ ! -f "$1" ]; then
    echo "Ошибка: файл $1 не существует"
    exit 1
fi

# Извлекаем идентификаторы и удаляем повторения
grep -o -E '\b[_a-zA-Z][_a-zA-Z0-9]*\b' "$1" | sort -u
_______________________________________________________________
chmod +x identifiers.sh
./identifiers.sh hello.c
